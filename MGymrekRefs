@Article{HuiStanglLaneEtAl2003,
  Title                    = {HnRNP L stimulates splicing of the eNOS gene by binding to variable-length CA repeats},
  Author                   = {Hui, J. and Stangl, K. and Lane, W. S. and Bindereif, A.},
