@Article{OliverSlashinskiWangEtAl2012,
  Title                    = {Balancing the risks and benefits of genomic data sharing: genome research participants' perspectives},
  Author                   = {Oliver, J. M. and Slashinski, M. J. and Wang, T. and Kelly, P. A. and Hilsenbeck, S. G. and McGuire, A. L.},
  Journal                  = {Public Health Genomics},
