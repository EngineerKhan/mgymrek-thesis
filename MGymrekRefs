@Article{RothenburgKoch-NolteRichEtAl2001,
  Title                    = {A polymorphic dinucleotide repeat in the rat nucleolin gene forms Z-DNA and inhibits promoter activity},
  Author                   = {Rothenburg, S. and Koch-Nolte, F. and Rich, A. and Haag, F.},
