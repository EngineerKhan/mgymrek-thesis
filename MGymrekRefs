@Article{FrumkinWasserstromItzkovitzEtAl2008,
  Title                    = {Cell lineage analysis of a mouse tumor},
  Author                   = {Frumkin, D. and Wasserstrom, A. and Itzkovitz, S. and Stern, T. and Harmelin, A. and Eilam, R. and Rechavi, G. and Shapiro, E.},
