@Article{GoodmanMundlosMuragakiEtAl1997,
  Title                    = {Synpolydactyly phenotypes correlate with size of expansions in HOXD13 polyalanine tract},
  Author                   = {Goodman, F. R. and Mundlos, S. and Muragaki, Y. and Donnai, D. and Giovannucci-Uzielli, M. L. and Lapi, E. and Majewski, F. and McGaughran, J. and McKeown, C. and Reardon, W. and Upton, J. and Winter, R. M. and Olsen, B. R. and Scambler, P. J.},
