@Article{PatwardhanHiattWittenEtAl2012,
  Title                    = {Massively parallel functional dissection of mammalian enhancers in vivo},
  Author                   = {Patwardhan, R. P. and Hiatt, J. B. and Witten, D. M. and Kim, M. J. and Smith, R. P. and May, D. and Lee, C. and Andrie, J. M. and Lee, S. I. and Cooper, G. M. and Ahituv, N. and Pennacchio, L. A. and Shendure, J.},
