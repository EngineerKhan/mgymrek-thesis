@Article{BrazileSchaepmanSchlapferEtAl2003,
  Title                    = {A Beowulf-style cluster processing concept for large volume hnaging sectroscopy data},
  Author                   = {Brazile, J. and Schaepman, M. and Schlapfer, D. and Kaiser, J. and Itten, K.},
