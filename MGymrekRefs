@Article{GuillonTirodeBoevaEtAl2009,
  Title                    = {The oncogenic EWS-FLI1 protein binds in vivo GGAA microsatellite sequences with potential transcriptional activation function},
  Author                   = {Guillon, N. and Tirode, F. and Boeva, V. and Zynovyev, A. and Barillot, E. and Delattre, O.},
  Journal                  = {PloS one},
