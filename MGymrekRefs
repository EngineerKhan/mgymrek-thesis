@Article{WillemsPaulHeideEtAl1990,
  Title                    = {Fimbrial phase variation in Bordetella pertussis: a novel mechanism for transcriptional regulation},
  Author                   = {Willems, R. and Paul, A. and van der Heide, H. G. and ter Avest, A. R. and Mooi, F. R.},
