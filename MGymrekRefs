@Article{MorrisAmriaKistner-GriffinEtAl2010,
  Title                    = {A GA microsatellite in the Fli1 promoter modulates gene expression and is associated with systemic lupus erythematosus patients without nephritis},
  Author                   = {Morris, E. E. and Amria, M. Y. and Kistner-Griffin, E. and Svenson, J. L. and Kamen, D. L. and Gilkeson, G. S. and Nowling, T. K.},
