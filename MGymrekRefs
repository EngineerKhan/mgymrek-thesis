@Article{ODushlaineEdwardsParkEtAl2005,
  Title                    = {Tandem repeat copy-number variation in protein-coding regions of human genes},
  Author                   = {O'Dushlaine, C. T. and Edwards, R. J. and Park, S. D. and Shields, D. C.},
  Journal                  = {Genome biology},
  Year                     = {2005},
  Number                   = {8},
