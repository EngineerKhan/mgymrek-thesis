@Article{SureshkumarTodescoSchneebergerEtAl2009,
  Title                    = {A genetic defect caused by a triplet repeat expansion in Arabidopsis thaliana},
  Author                   = {Sureshkumar, S. and Todesco, M. and Schneeberger, K. and Harilal, R. and Balasubramanian, S. and Weigel, D.},
