@Article{Marichal-HernandezRosaRodriguez-Ramos2006a,
  Title                    = {A Multi Output Fast Fourier Transform Implementation on Graphics Hardware for Optical Simulation},
  Author                   = {Marichal-Hernandez, J. G. and Rosa, F. and Rodriguez-Ramos, J. M.},
  Journal                  = {International Mediterranean Modelling Multiconference 2006},
  Year                     = {2006},
  Pages                    = {593-598},
  Abstract                 = {In a previous work we have described the key factors in the selection of an efficient variant of the fast Fourier transform algorithm to take advantage of Graphics Processing Units (hereafter, GPUs) on some simulations.
In this work we explore the more efficient way to exploit Mutiple Render Target novel feature of this hardware. By applying a simultaneous collapse of index summations along both dimensions we will achieve a 2D FFT implementation that is 6 times faster than FFTW running on a high-end CPU.
This is a remarkable speedup, specially on those simulations concerning visualization of results, as is the case in our motivating problem: the simulation of light transport through a paraxial lens.},
