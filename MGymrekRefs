@Article{LuWallrathGranokEtAl1993,
  Title                    = {(CT)n (GA)n repeats and heat shock elements have distinct roles in chromatin structure and transcriptional activation of the Drosophila hsp26 gene},
  Author                   = {Lu, Q. and Wallrath, L. L. and Granok, H. and Elgin, S. C.},
