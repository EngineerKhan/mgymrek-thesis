@Article{BrinkmannKlintscharNeuhuberEtAl1998,
  Title                    = {Mutation rate in human microsatellites: influence of the structure and length of the tandem repeat},
  Author                   = {Brinkmann, B. and Klintschar, M. and Neuhuber, F. and Huhne, J. and Rolf, B.},
  Journal                  = {American journal of human genetics},
  Year                     = {1998},
  Number                   = {6},
  Pages                    = {1408-15},
  Volume                   = {62},
  Abstract                 = {In 10,844 parent/child allelic transfers at nine short-tandem-repeat (STR) loci, 23 isolated STR mismatches were observed. The parenthood in each of these cases was highly validated (probability >99.97%). The event was always repeat related, owing to either a single-step mutation (n=22) or a double-step mutation (n=1). The mutation rate was between 0 and 7 x 10(-3) per locus per gamete per generation. No mutations were observed in three of the nine loci. Mutation events in the male germ line were five to six times more frequent than in the female germ line. A positive exponential correlation between the geometric mean of the number of uninterrupted repeats and the mutation rate was observed. Our data demonstrate that mutation rates of different loci can differ by several orders of magnitude and that different alleles at one locus exhibit different mutation rates.},
