@Article{StrangerNicaForrestEtAl2007,
  Title                    = {Population genomics of human gene expression},
  Author                   = {Stranger, B. E. and Nica, A. C. and Forrest, M. S. and Dimas, A. and Bird, C. P. and Beazley, C. and Ingle, C. E. and Dunning, M. and Flicek, P. and Koller, D. and Montgomery, S. and Tavare, S. and Deloukas, P. and Dermitzakis, E. T.},
