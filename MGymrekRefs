@Article{HeGitschierZerjalEtAl2009,
  Title                    = {Geographical affinities of the HapMap samples},
  Author                   = {He, M. and Gitschier, J. and Zerjal, T. and de Knijff, P. and Tyler-Smith, C. and Xue, Y.},
  Journal                  = {PLoS One},
