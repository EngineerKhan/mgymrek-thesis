@Article{ThomsonPritchardShenEtAl2000,
  Title                    = {Recent common ancestry of human Y chromosomes: evidence from DNA sequence data},
  Author                   = {Thomson, R. and Pritchard, J. K. and Shen, P. and Oefner, P. J. and Feldman, M. W.},
  Journal                  = {Proc Natl Acad Sci U S A},
  Year                     = {2000},
