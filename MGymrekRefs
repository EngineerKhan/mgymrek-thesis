  Author                   = {Erlich, Y. and Edvardson, S. and Hodges, E. and Zenvirt, S. and Thekkat, P. and Shaag, A. and Dor, T. and Hannon, G. J. and Elpeleg, O.},
  Journal                  = {Genome Research},
