@Article{McGuireOliverSlashinskiEtAl2011,
  Title                    = {To share or not to share: a randomized trial of consent for data sharing in genome research},
  Author                   = {McGuire, A. L. and Oliver, J. M. and Slashinski, M. J. and Graves, J. L. and Wang, T. and Kelly, P. A. and Fisher, W. and Lau, C. C. and Goss, J. and Okcu, M. and Treadwell-Deering, D. and Goldman, A. M. and Noebels, J. L. and Hilsenbeck, S. G.},
